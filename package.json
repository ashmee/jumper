{
  "name": "jumper-phaser-ts",
  "version": "1.0.0",
  "description": "A template project for Phaser 3 using Parceljs",
  "scripts": {
    "start": "parcel src/index.html -p 8000",
    "build": "parcel build src/index.html --out-dir dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx"
  },
  "author": "ashmee",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "minimist": ">=1.2.5",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.5.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "phaser": "^3.52.0"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "src/assets",
    "watcherGlob": "**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashmee/jumper.git"
  },
  "bugs": {
    "url": "https://github.com/ashmee/jumper/issues"
  },
  "homepage": "https://github.com/ashmee/jumper#readme"
}
